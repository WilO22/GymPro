# Firebase Configuration for Development and Production
# Copy this file to .env.local and fill in your actual Firebase credentials
# NEVER commit .env.local to version control

# Firebase API Key
NG_APP_FIREBASE_API_KEY=YOUR_API_KEY_HERE

# Firebase Auth Domain
NG_APP_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com

# Firebase Project ID
NG_APP_FIREBASE_PROJECT_ID=your-project-id

# Firebase Storage Bucket
NG_APP_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app

# Firebase Messaging Sender ID
NG_APP_FIREBASE_MESSAGING_SENDER_ID=123456789

# Firebase App ID
NG_APP_FIREBASE_APP_ID=1:123456789:web:abc123
